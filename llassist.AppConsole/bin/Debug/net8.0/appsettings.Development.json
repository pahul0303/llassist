{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "LlassistAppDatabase": "Host=localhost;Database=llassistdb;Username=postgres;Password=NotSoGoodPassword;"
  },
  "Queue": {
    "QueueName": "llassistqueue",
    "EnableDelayedProcessing": true,
    "EnableHeartBeat": true,
    "EnableMessageExpiration": false,
    "EnableStatus": true,
    "ConsumerWorkerCount": 5,
    "HeartBeatUpdateTime": "sec(*%10)",
    "HeartBeatMonitorTime": 15,
    "HeartBeatTime": 35,
    "MessageExpirationEnabled": true,
    "MessageExpirationMonitorTime": 20,
  },
}
